{
    "redirects": [
        {
            "source"     : "/search",
            "destination": "/",
            "permanent"  : false
        },
        {
            "source": "/(.*)",
            "has": [
                {
                    "type" : "host",
                    "value": "homegrownspeed.shop"
                }
            ],
            "destination": "https://homegrownspeed.shop/$1",
            "permanent"  : true
        }
    ]
}